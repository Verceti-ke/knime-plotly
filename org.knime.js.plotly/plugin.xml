<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.knime.dynamic.node.generation.dynamicNodes">
      <dynamicNodeFolder
            description="KNIME base implementations of dynamically created JavaScript enabled nodes"
            function="js"
            name="KNIME base implementations of dynamically created JavaScript enabled nodes"
            path="nodes/">
      </dynamicNodeFolder>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="nodes/js.png"
            level-id="plotly"
            locked="true"
            name="Plotly"
            path="/labs/js-views">
     </category>
    </extension>
   <extension
         point="org.knime.js.core.webResources">
      <webResourceBundle
            debug="true"
            name="3D Scatter Plot"
            usesDefine="false"
            version="1.0.0"
            webResourceBundleID="org.knime.js.plotly.scatterPlot3DBundle">
         <webResource
               relativePathSource="nodes/scatterPlot3D/scatterPlot3D.js"
               relativePathTarget="org/knime/js/plotly/scatterPlot3D/scatterPlot3D.js">
         </webResource>
         <webResource
               relativePathSource="nodes/scatterPlot3D/scatterPlot3D.css"
               relativePathTarget="org/knime/js/plotly/scatterPlot3D/scatterPlot3D.css">
         </webResource>
         <importResource
               relativePath="org/knime/js/plotly/scatterPlot3D/scatterPlot3D.js"
               type="JAVASCRIPT">
         </importResource>
         <importResource
               relativePath="org/knime/js/plotly/scatterPlot3D/scatterPlot3D.css"
               type="CSS">
         </importResource>
         <webDependency
               webResourceBundleID="plotly.js-1.47.4">
         </webDependency>
         <webDependency
               webResourceBundleID="plotly.js-1.47.4_Debug">
         </webDependency>
      </webResourceBundle>
   </extension>

</plugin>
